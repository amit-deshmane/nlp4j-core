<configuration>
	<tsv>
        <column index="1" field="form"/>
        <column index="2" field="lemma"/>
        <column index="3" field="pos"/>
        <column index="4" field="feats"/>
        <column index="5" field="nament"/>
    </tsv>

    <lexica>
        <word_clusters field="uncapitalized_simplified_word_form">edu/emory/mathcs/nlp/english/lexica/en-brown-clusters-simplified-uncapitalized.xz</word_clusters>
        <named_entity_gazetteers field="simplified_word_form">edu/emory/mathcs/nlp/english/lexica/en-ner-gazetteers-simplified.xz</named_entity_gazetteers>
    </lexica>

    <optimizer>
        <algorithm>adagrad-mini-batch</algorithm>
        <l1_regularization>0.000015</l1_regularization>
        <learning_rate>0.02</learning_rate>
        <max_epochs>20</max_epochs>
        <batch_size>5</batch_size>
        <roll_in>0.95</roll_in>
        <bias>0</bias>
    </optimizer>
</configuration>
